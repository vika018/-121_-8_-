<Window x:Class="ИС121_ПР8_Фамутдинова.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ИС121_ПР8_Фамутдинова"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <StackPanel>
            <Button Width="200" Height="80"
                    Style="{StaticResource St1}"/>
            
            <Button Width="200" Height="80" Content="День"
                    Style="{StaticResource SBlue}"/>
            
            <Button Width="200" Height="80" Content="Вечер"
                    Style="{StaticResource SCircle}"/>
            
            <Button Width="200" Height="80" Content="Ночь"
                    Style="{StaticResource SClassic}"/>
        </StackPanel>
    </Grid>
</Window>
